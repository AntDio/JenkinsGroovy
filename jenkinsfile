node {
    stage('Build') {
    
    	echo pwd()
    
    	sh 'dotnet --version'
    	sh(script: "dotnet build ConsoleApp1/ConsoleApp1.sln", returnStdout: true)
        echo 'Building....'
        echo 'Building New Feature'
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}